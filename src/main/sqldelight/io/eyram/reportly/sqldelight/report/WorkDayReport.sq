import models.WorkDay;



CREATE TABLE report (
  workWeek INTEGER AS Int NOT NULL,
  workDay TEXT AS WorkDay NOT NULL,
  report TEXT,
  timeOn TEXT,
  timeOff TEXT,
  PRIMARY KEY (workWeek, workDay)
);


selectAll:
SELECT *
FROM report
GROUP BY workWeek;

commitReport:
INSERT INTO report(workWeek,workDay,report,timeOn,timeOff)
VALUES ?;